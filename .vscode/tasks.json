{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "0.1.0",
    "command": "bash",
    "isShellCommand": true,
    "args": [ "-c" ],
    "showOutput": "always",
    "tasks": [{
        "taskName": "Gulp Default Task",
        "suppressTaskName": true,
        "args": [ "gulp" ],
        "isBuildCommand": true,
        "problemMatcher": "$gulp-tsc",
        "isWatching": false,
        "showOutput": "always"
    }]
}